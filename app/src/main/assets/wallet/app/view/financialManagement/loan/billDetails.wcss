.ga-new-page{
    background-color: #fff;
}
.ga-line{
    height: 16px;
    background-color: #f8f8f8;
}
.ga-bill-detail{
    padding: 30px;
}
.ga-box1{
    display: flex;
    justify-content: space-between;
    font-size: 24px;
    color: #203260;
    line-height: 33px;
}
.ga-bill-value{
    font-size: 64px;
    color: #212C68;
    letter-spacing: 0;
    text-align: center;
    line-height: 90px;
    margin-top: 70px;
}
.ga-bill-status{
    font-size: 28px;
    color: #212C68;
    letter-spacing: 0;
    text-align: center;
    line-height: 40px;
    margin-top: 15px;
}
.ga-bill-msg{
    display: flex;
    justify-content: center;
    margin-top: 70px;
    padding-top: 40px;
    border-top: 1px solid #e5e5e5;
    font-size: 28px;
    line-height: 40px;
}
.ga-left{
    display: flex;
    flex-direction: column;
    align-items: flex-end;
}
.ga-right{
    display: flex;
    flex-direction: column;
}
.ga-label{
    color: #8E96AB;
    margin-right: 30px;
    margin-bottom: 16px;
}
.ga-value{
    color: #203260;
    margin-bottom: 16px;
}
.ga-btn-bottom{
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgba(255,255,255,0.80);
    height: 98px;
    border-top: 1px solid #e5e5e5;
    font-size: 28px;
    color: #212C68;
    line-height: 40px;
    display: flex;
    justify-content: center;
    align-items: center;
}